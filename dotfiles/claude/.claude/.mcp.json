{
  "mcpServers": {
    "mcp-atlassian": {
      "type": "sse",
      "url": "http://localhost:9000/sse"
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/sse"
    },
    "firecrawl": {
      "type": "sse",
      "url": "http://localhost:3015/sse"
    },
    "bigquery": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-bigquery",
        "--project",
        "${BIGQUERY_PROJECT}",
        "--location",
        "${BIGQUERY_LOCATION}",
        "--key-file",
        "${BIGQUERY_KEY_FILE}"
      ]
    }
  }
}
