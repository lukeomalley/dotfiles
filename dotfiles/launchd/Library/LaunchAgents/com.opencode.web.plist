<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.opencode.web</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>-l</string>
        <string>/Users/luke/bin/start-opencode-web</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/luke/.local/log/opencode-web.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/luke/.local/log/opencode-web.err</string>

    <key>WorkingDirectory</key>
    <string>/Users/luke</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
